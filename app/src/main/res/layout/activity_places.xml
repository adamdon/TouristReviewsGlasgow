<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/places_page1"
    tools:context="uk.ac.cityofglasgowcollege.gccadamdon.Places">

    <ImageButton
        android:id="@+id/btnTitleHome"
        android:layout_width="97dp"
        android:layout_height="63dp"
        android:layout_x="0dp"
        android:layout_y="0dp"
        android:background="@null"
        android:onClick="goToActivityHomeOne"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/titlebar_home_non_clicked"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-1dp" />

    <ImageButton
        android:id="@+id/btnTitlePlaces"
        android:layout_width="99dp"
        android:layout_height="65dp"
        android:layout_x="97dp"
        android:layout_y="0dp"
        android:background="@null"
        android:onClick="goToActivityPlaces"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/titlebar_places_clicked"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="-1dp" />

    <ImageButton
        android:id="@+id/btnTitleSettings"
        android:layout_width="99dp"
        android:layout_height="63dp"
        android:layout_x="196dp"
        android:layout_y="0dp"
        android:background="@null"
        android:onClick="goToActivitySettings"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/titlebar_settings_non_clicked"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="-1dp" />

    <ImageButton
        android:id="@+id/btnTitleDots"
        android:layout_width="91dp"
        android:layout_height="63dp"
        android:layout_x="295dp"
        android:layout_y="0dp"
        android:background="@null"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/titlebar_contextdots_non_clicked"
        tools:layout_editor_absoluteX="292dp"
        tools:layout_editor_absoluteY="-1dp" />

    <ImageButton
        android:id="@+id/btn_Palaces_Kelvin"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:layout_x="0dp"
        android:layout_y="231dp"
        android:background="@null"
        android:onClick="goToActivityPlaceKelvingrove"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/kg_art_gall_and_mus_button" />

    <ImageButton
        android:id="@+id/btn_Palaces_Lighthouse"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:layout_x="1dp"
        android:layout_y="306dp"
        android:background="@null"
        android:onClick="goToActivityPlaceLighthouse"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/the_lighthouse_button" />

    <ImageButton
        android:id="@+id/btn_Palaces_Buchanan"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:layout_x="0dp"
        android:layout_y="392dp"
        android:background="@null"
        android:onClick="goToActivityPlaceBuchanan"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/buch_st_button" />

    <ImageButton
        android:id="@+id/btn_Palaces_Green"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:layout_x="0dp"
        android:layout_y="480dp"
        android:background="@null"
        android:onClick="goToActivityPlaceGlasgowgreen"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/glas_green_button" />
</AbsoluteLayout>
